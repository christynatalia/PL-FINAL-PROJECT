    <children>
        <Button fx:id="BtnSAdd" layoutX="190.0" layoutY="277.0" mnemonicParsing="false" onAction="#CheckData" text="Add Data to Sales" />
        <TextField fx:id="TFDProdName" layoutX="139.0" layoutY="125.0" />
        <TextField fx:id="TFDQty" layoutX="139.0" layoutY="173.0" />
        <TextField fx:id="TFDProdID" layoutX="139.0" layoutY="76.0" />
        <Label layoutX="36.0" layoutY="81.0" text="Product ID" />
        <Label layoutX="36.0" layoutY="130.0" text="Product Name" />
